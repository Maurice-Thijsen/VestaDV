		parameter<Eur>			Ki_TEO_vast     :=		Schuiven/KostenMin * Ki_TEO_vast_min + Schuiven/KostenMax * Ki_TEO_vast_max;
		parameter<Eur_kW>		Ki_TEO_var      :=		Schuiven/KostenMin * Ki_TEO_var_min  + Schuiven/KostenMax * Ki_TEO_var_max;
	
