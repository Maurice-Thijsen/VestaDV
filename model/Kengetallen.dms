		parameter<Eur_kw>		Ki_HR_ketel_U   :=		   79.6581 [Eur_kw]    ,	Descr = "bespaarde investering voor gasketels utiliteit", Source = "Kengetallen Arcadis 2018 - 876.24 per 11kw ketel";
		
